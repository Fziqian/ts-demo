{
    "defaultSeverity": "warn",
    "include":[
        "./src/*/*"
    ],
    "exclude":[
        "node_modules"
    ],
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotes": [
            0,
            "single"
        ],
        "semi": [
            0,
            "always"
        ],
        "no-console":false,
        "one-variable-per-declaration":false,
        "prefer-const":false
    },
    "rulesDirectory": []
}